<% include ./partials/header %>

<h1>INDEX!</h1>

<% blogs.forEach(function(blog) { %>
  <h2><%= blog.title %></h2>
  <img src="<%= blog.image %>"/>
  <span><%= blog.created %></span>
  <p>
    <%= blog.body.substring(0, 100) %>...
    <a href="blogs/<%= blog._id %>"> Read More</a>
  </p>
<% }); %>

<% include ./partials/footer %>
